<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="sequence_payment_customer_debit" model="ir.sequence">
            <field name="name">Payments customer debit sequence</field>
            <field name="code">account.payment.customer.debit</field>
            <field name="prefix">CUST.DIN/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>